model Vocabulary {
  id                  Int      @id @default(autoincrement())
  word                String
  word_audio_url      String?
  meaning             String
  meaning_audio_url   String?
  examples            Json[]
  context             String?
  created_at          DateTime @default(now())
  updated_at          DateTime?
  deleted_at          DateTime?
  // Define a many-to-one relationship with User
  user                User     @relation(fields: [user_id], references: [id])
  user_id             Int

  vocabulary_topic_id Int?
  vocabulary_topic  VocabularyTopic? @relation(fields: [vocabulary_topic_id], references: [id])
  @@index([vocabulary_topic_id])
}