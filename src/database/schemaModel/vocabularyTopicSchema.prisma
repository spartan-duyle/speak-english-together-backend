model VocabularyTopic {
  id Int @id @default(autoincrement())
  name String
  description String?
  vocabularies  Vocabulary[]
  user_id        Int
  user          User @relation(fields: [user_id], references: [id])
  created_at DateTime @default(now())
  updated_at DateTime?
  deleted_at DateTime?
  @@unique([name, user_id, deleted_at])
}